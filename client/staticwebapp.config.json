{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "/swarmspace/*", "/tasks/*", "*.{css,js,json,png,jpg,jpeg,gif,ico,svg}"]
  },
  "routes": [
    {
      "route": "/tasks",
      "redirect": "/tasks/"
    },
    {
      "route": "/swarmspace",
      "redirect": "/swarmspace/"
    },
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "platform": {
    "apiRuntime": "node:20"
  }
}
